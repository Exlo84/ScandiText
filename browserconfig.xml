<?xml version="1.0" encoding="utf-8"?>
<browserconfig>
    <msapplication>
        <tile>
            <TileColor>#3498db</TileColor>
        </tile>
        <badge>
            <polling-uri src="notifications/pollForBadge1.xml"/>
            <polling-uri2 src="notifications/pollForBadge2.xml"/>
            <polling-uri3 src="notifications/pollForBadge3.xml"/>
            <polling-uri4 src="notifications/pollForBadge4.xml"/>
            <polling-uri5 src="notifications/pollForBadge5.xml"/>
            <frequency>30</frequency>
            <cycle>1</cycle>
        </badge>
        <notification>
            <polling-uri src="notifications/pollForNotification1.xml"/>
            <polling-uri2 src="notifications/pollForNotification2.xml"/>
            <polling-uri3 src="notifications/pollForNotification3.xml"/>
            <polling-uri4 src="notifications/pollForNotification4.xml"/>
            <polling-uri5 src="notifications/pollForNotification5.xml"/>
            <frequency>30</frequency>
            <cycle>1</cycle>
        </notification>
    </msapplication>
</browserconfig>
